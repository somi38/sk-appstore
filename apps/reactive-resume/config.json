{
    "name": "Reactive Resume",
    "id": "reactive-resume",
    "version": "3.11.0",
    "available": true,
    "port": 3000,
    "exposable": true,
    "dynamic_config": true,
    "short_desc": "A free and open source resume builder.",
    "description": "Reactive Resume is a free and open source resume builder that simplifies the process of creating, updating, and sharing your resume. You can import data from LinkedIn, use multiple templates, and export your resume as a PDF.",
    "author": "Amruth Pillai",
    "source": "https://github.com/AmruthPillai/Reactive-Resume",
    "website": "https://rxresu.me/",
    "categories": [
        "productivity",
        "utilities"
    ],
    "tipi_version": 3,
    "supported_architectures": [
        "arm64",
        "amd64"
    ],
    "form_fields": [
        {
            "name": "postgres_password",
            "label": "PostgreSQL Password",
            "type": "text",
            "required": true,
            "default_value": "changeme"
        },
        {
            "name": "minio_root_user",
            "label": "MinIO Root User",
            "type": "text",
            "required": true,
            "default_value": "reactive-resume"
        },
        {
            "name": "minio_root_password",
            "label": "MinIO Root Password",
            "type": "text",
            "required": true,
            "default_value": "changeme"
        }
    ]
}
