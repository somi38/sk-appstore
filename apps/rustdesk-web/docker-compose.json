{
  "_comment": ""schemaVersion": 2",
  "_comment": ""$schema": "https://schemas.runtipi.io/dynamic-compose.json"",
  "services": [
    {
      "name": "rustdesk-web",
      "image": "pmietlicki/rustdesk-web-client:v1",
      "isMain": true,
      "internalPort": "5000",
      "restart": "unless-stopped",
      "environment": [
        {
          "key": "RUSTDESK_SERVER",
          "value": "${RUSTDESK_SERVER_HOST}"
        }
      ]
    }
  ]
}
